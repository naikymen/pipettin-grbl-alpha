{
  "env": {
    "es6": true,
    "mocha": false,
    "node": true
  },
  "extends": "semistandard",
  "plugins": [
    "standard"
  ],
  "rules": {
    "no-unused-vars": ["warn", { "vars": "all", "args": "after-used" }],
    "no-var": "error",
    "prefer-const": "error",
    "no-else-return": "error",
    "object-shorthand": ["error", "properties"]
  }
}
